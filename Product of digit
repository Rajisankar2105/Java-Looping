import java.util.Scanner;
public class Main {
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        System.out.print("Enter a number:");
        int num=sc.nextInt();
        int pro=1;
        while(num!=0)
{
   pro*=num%10;
    num/=10;
}   
        System.out.println("Product:"+pro);
    }
}

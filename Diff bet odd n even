import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter a number: ");
        int num=sc.nextInt();
        int evenSum=0,oddSum=0;
        while(num>0){
            int d=num%10;
            if(d%2==0)evenSum +=d;
            else oddSum += d;
            num/=10;
        }
        System.out.println("Difference:"+Math.abs(evenSum-oddSum));
         }
}
